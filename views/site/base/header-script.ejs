<script>
    var shoppingCartItem = [];
    if (sessionStorage["shopping-cart-items"] != null) {
        shoppingCartItem = JSON.parse(sessionStorage["shopping-cart-items"].toString());
        var cart = shoppingCartItem.length;
        if (cart > 0) {
            $('#bage-cart').empty().text('('+cart+')');
        } else {
            $('#bage-cart').empty();
        }
    }else{
        $('#bage-cart').empty();
    }
</script>