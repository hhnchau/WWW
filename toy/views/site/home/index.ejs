    <style>
        .section{
            padding-top: 0.01rem !important;
            padding-bottom: 0px !important;
        }

        .content {
            padding: 0px !important;
            cursor: pointer;
        }

        .ellipsis {
            overflow: hidden;
            text-overflow: ellipsis;
            display: -webkit-box;
            -webkit-line-clamp: 1;
            -webkit-box-orient: vertical
        }

        h4 {
            font-size: 2.0rem !important;
