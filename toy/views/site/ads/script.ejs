<script>
    var countDown = 6;
    $("#btn").hide();
    var x = setInterval(function () {

        countDown--;
        document.getElementById("link").innerHTML = countDown;

        if (countDown < 0) {
            clearInterval(x);

            $("#loading").hide();
            $("#link").hide();
            $("#btn").show();
            '<%if(vip.length == 0){%>'
                $("#find").text("Không tìm thấy liên kết");
                $("#btn").text("Trang Chủ");  
            '<%}else{%>'
            '<% vip.forEach(function(item, index){%>';
                $("#find").text("Liên kết đã sẵn sàng");
                $("#btn").text("Play");  
            '<%})%>'
            '<%}%>'
        }
    }, 1000);
</script>